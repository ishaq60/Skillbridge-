{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/api/find-matches/route.js", "app/api/extract-skills/route.js", "components/swaping/user-profile.jsx", "components/swaping/theme-provider.jsx", "components/swaping/search-users.jsx", "components/swaping/profile-editor.jsx", "components/swaping/ml-metrics.jsx", "components/swaping/matches-list.jsx", "app/skillexchange/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
